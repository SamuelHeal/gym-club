<main class="body feed"> 
    <h1 class="blogHeading">Blogs</h1>
    {{#each blogs as |blog| }}
    
      <section class="individual-blog" onclick="location.href='/blog/{{blog.id}}'">
        
        <h1 class="blog-heading">{{blog.title}}</h1>
        <div class="blog-para datey">
          <p>{{format_time blog.date_created}} | <a class="aStyle" href="/user/{{blog.user.id}}">{{blog.user.username}}</a></p>
        </div>
        <div class="blog-para descy">
          <p>{{blog.description}}</p>
        </div>
        
      </section>
      
    
    {{/each}}
  </main>
